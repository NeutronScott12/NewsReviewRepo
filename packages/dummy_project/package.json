{
    "name": "dummy_project",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "migrate:generate": "npm run prisma:migrate && npm run prisma:generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:generate": "npx prisma generate",
        "prisma:reset": "npx prisma migrate reset",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:build": "docker-compose build",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npx cross-env NODE_ENV=development && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "npx cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@casl/ability": "^5.4.4",
        "@casl/prisma": "^1.0.2",
        "@nestjs/apollo": "^10.0.17",
        "@nestjs/class-validator": "^0.13.4",
        "@nestjs/common": "9.0.3",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "9.0.3",
        "@nestjs/graphql": "^10.0.18",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mercurius": "^10.0.17",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.3",
        "@prisma/client": "^4.0.0",
        "@thelasthurrah/the-last-hurrah-shared": "^0.1.26",
        "apollo-server-cache-redis": "^3.3.1",
        "apollo-server-core": "^3.10.0",
        "apollo-server-express": "^3.10.0",
        "cache-manager": "^4.1.0",
        "cache-manager-redis-store": "^2.0.0",
        "cross-env": "^7.0.3",
        "graphql": "^16.5.0",
        "graphql-type-json": "^0.3.2",
        "joi": "^17.6.0",
        "mercurius": "^10.1.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.3.0",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.3",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.5",
        "@types/joi": "^17.2.3",
        "@types/node": "^18.0.4",
        "@types/passport-jwt": "^3.0.6",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^28.1.3",
        "lerna": "^5.1.8",
        "prettier": "^2.7.1",
        "prisma": "^4.0.0",
        "source-map-support": "^0.5.20",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.6",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
